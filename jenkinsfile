pipeline {
    agent 192.168.224.134
    stages {
        stage('Build') {
            steps {
               sh pre-checks.sh
            }
        }
        stage('Test') {
            steps {
              sh patching.sh
            }
        }
        stage('post patching') {
            steps {
                sh post-checks.sh
            }
        }
    }
    post {
        always {
            echo "Patching has been completed successfully"
        }
    }
}
